{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4",
        "albertcht/invisible-recaptcha": "^1.9",
        "baum/baum": "v2.x-dev",
        "cviebrock/eloquent-sluggable": "^7.0",
        "doctrine/dbal": "^2.10",
        "eduardokum/laravel-mail-auto-embed": "^1.0",
        "fideloper/proxy": "^4.4",
        "graham-campbell/markdown": "^13.1",
        "guzzlehttp/guzzle": "^6.5",
        "htmlmin/htmlmin": "^7.0",
        "intervention/image": "^2.5",
        "ipunkt/laravel-analytics": "^4.0",
        "itsgoingd/clockwork": "^4.1",
        "jenssegers/date": "^4.0.0",
        "laravel/framework": "^7.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.4",
        "laravelcollective/html": "^6.0",
        "laravelium/feed": "7.0.1",
        "laravelium/sitemap": "7.0.1",
        "msurguy/honeypot": "^1.1",
        "nesbot/carbon": "^2.40",
        "nikic/php-parser": "^4.10",
        "predis/predis": "^1.1",
        "symfony/yaml": "^5.1",
        "wpb/string-blade-compiler": "^6.0"
    },
    "require-dev": {
        "filp/whoops": "^2.7",
        "fzaninotto/faker": "^1.9",
        "laravel/browser-kit-testing": "^6.1",
        "laravel/dusk": "^6.6",
        "mockery/mockery": "^1.4",
        "nunomaduro/collision": "^4.2",
        "phpunit/phpunit": "^8.5",
        "staudenmeir/dusk-updater": "^1.2",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Utils/utils.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php",
            "tests/BrowserKitTestCase.php"
        ],
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
